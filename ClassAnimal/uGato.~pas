unit uGato;

interface

uses
  uAnimal;


type
  TGato = class(TAnimal)
  private
    FEspecie: string;
  public
    constructor Create; override;
    function Hablar: string;
  end;

implementation

constructor TGato.Create;
begin
  inherited;
  FEspecie := 'Felino';
end;

function TGato.Hablar: string;
begin
  Result := 'Mauyar';
  // TODO -cMM: TGato.Hablar default body inserted
end;

end.
